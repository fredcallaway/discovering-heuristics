	Issn = {1097-6256},
	Journal = {Nature neuroscience},
	Number = {12},
	Pages = {1704--11},
	Pmid = {16286932},
	Title = {{Uncertainty-based competition between prefrontal and dorsolateral striatal systems for behavioral control.}},
